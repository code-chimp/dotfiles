{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "camelcase": 2,
    "curly": [2, "all"],
    "default-case": 0,
    "eqeqeq": 2,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-debugger": 2,
    "no-eq-nul": 0,
    "no-fallthrough": 2,
    "no-floating-decimal": 1,
    "no-labels": 2,
    "no-multi-spaces": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "quotes": [2, "single"],
    "strict": [2, "global"],
    "use-isnan": 2,
    "wrap-iife": 2,
    "yoda": [2, "never", {"exceptRange": true}]
  }
}
